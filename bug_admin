            or 'filter_condition_callback' => array($this,'_filterNames'),
                  protected function _filterNames($collection, $column)
                {
                    $value = $column->getFilter()->getValue();
                    if($value) {
                        $collection->getSelect()->where("`sales/order_item`.`name` LIKE '%$value%'");
                    }
                    return $this;
                }
